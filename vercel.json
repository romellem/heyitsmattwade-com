{
	"routes": [
		{
			"src": "/resume",
			"status": 301,
			"headers": { "Location": "/pdfs/Matthew%20Wade%20-%20Resume.pdf" }
		},
		{
			"src": "/pdf/(.*)",
			"status": 301,
			"headers": { "Location": "/pdfs/$1" }
		},
		{
			"src": "/img/(.*)",
			"status": 301,
			"headers": { "Location": "/images/$1" }
		}
	]
}
